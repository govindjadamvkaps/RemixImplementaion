{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -i ./styles/app.css -o ./app/styles/app.css -m",
    "dev": "concurrently \"npm run dev:css\"  \"remix dev\"",
    "dev:css": "tailwindcss -i ./styles/app.css -o ./app/styles/app.css -w",
    "start": "remix-serve build",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@remix-run/css-bundle": "^1.17.1",
    "@remix-run/node": "^1.17.1",
    "@remix-run/react": "^1.17.1",
    "@remix-run/serve": "^1.17.1",
    "@remix-validated-form/with-zod": "^2.0.6",
    "axios": "^1.4.0",
    "flowbite": "^1.6.6",
    "http-status-codes": "^2.2.0",
    "isbot": "^3.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.0",
    "react-toastify": "^9.1.3",
    "remix-validated-form": "^5.0.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.17.1",
    "@remix-run/eslint-config": "^1.17.1",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.0",
    "eslint": "^8.38.0",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
